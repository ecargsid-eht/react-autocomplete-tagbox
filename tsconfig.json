{
  "compilerOptions":{
    "target": "es5",
    "lib": ["dom", "dom.iterable","esnext"],
    "allowJs": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noFallthroughCasesInSwitch": true,
    "rootDir": "./src",
  },
  "include": ["src"]
}